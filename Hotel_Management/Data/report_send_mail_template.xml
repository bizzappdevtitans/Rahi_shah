<?xml version="1.0" encoding="utf-8"?> 
 <odoo>
    <data noupdate="1">
        <record id="room_report_mail_template" model="mail.template">
            <field name="name">Report Send:By Email</field>
            <field name="model_id" ref="Hotel_Management.model_room_reservation"/>
            <field name="email_from">hotel@gmail.com</field>
            <field name="email_to">{{object.email}}</field>
            <field name="subject">Room Reservation Report</field>
           
            <field name="body_html" type="html">
                <p>
                    Dear <t t-out="object.guest_name.name"/>
                    <br/><br/>
                    Here is your Report with reference <t t-out="object.room_booking"/> <br/><br/>
                    If you have Query feel free to ask 
                </p>
            </field>
            <field name="report_template" ref="report_hotel_management"/> 
            <field name="report_name">Reservation report {{object.room_booking}} </field> 
        </record>
    </data>
</odoo>