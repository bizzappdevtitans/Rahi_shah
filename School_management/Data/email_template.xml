<?xml version="1.0" encoding="utf-8"?> 
 <odoo>
    <data noupdate="1">
        <record id="student_birthday_mail_template" model="mail.template">
            <field name="name">EMAIL birthday </field>
            <field name="model_id" ref="School_management.model_school_student"/>
            <field name="email_from">rahi@gmail.com</field>
            <field name="email_to">{{object.Email}}</field>
            <field name="subject">Birthday Wish</field>
            <field name="body_html" type="html">
                <p>
                Dear <t t-out="object.name"/>
                <br/>
                <br/>
                Happy birthday
                <br/>
                </p>
                <br/>
                <t t-out="object.name"/>
            </field>
        </record> 
    </data>
</odoo> 

