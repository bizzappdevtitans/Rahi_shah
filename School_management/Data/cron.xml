<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

   <record model="ir.cron" id="send_birthday_cron">
            <field name="name">Birthday Cron</field>
            <field name="model_id" ref="model_school_student" />
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">model._cron_send_birthday()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
        </record>

     <record model="ir.cron" id="send_message_cron">
            <field name="name">Assignment Not submitted Cron</field>
            <field name="model_id" ref="model_school_submit" />
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">model.cron_send_message()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
        </record>
     </data>
  </odoo>